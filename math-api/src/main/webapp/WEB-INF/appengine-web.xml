<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
    <application>classical-mechanics</application>
    <version>${appengine.app.version}</version>
    <threadsafe>true</threadsafe>
    
    <system-properties>
        <property name="java.util.logging.config.file" value="WEB-INF/classes/log4j.properties"/>
        <property name="clojure.core.loading-verbosely" value="true"/>
        <property name="clojure.core.warn-on-reflection" value="true"/>
    </system-properties>
</appengine-web-app>
