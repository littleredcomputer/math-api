<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>hello</servlet-name>
        <servlet-class>net.littleredcomputer.math.api.HelloServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>howdy</servlet-name>
        <servlet-class>net.littleredcomputer.math.api.HowdyServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>foo</servlet-name>
        <servlet-class>net.littleredcomputer.math.api.foo</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>hello</servlet-name>
        <url-pattern>/hello</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>howdy</servlet-name>
        <url-pattern>/howdy</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>foo</servlet-name>
        <url-pattern>/foo/*</url-pattern>
    </servlet-mapping>
</web-app>
